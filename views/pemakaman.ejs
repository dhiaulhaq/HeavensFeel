<%- include('_nav', { page: 'pemakaman' }) %>

        <!-- Pemakaman List Start -->
        <div class="container-xxl py-5">
            <div class="container">
                <div class="row g-0 gx-5 align-items-end">
                    <div class="text-start mx-auto mb-5 wow slideInLeft" data-wow-delay="0.1s">
                        <h1 class="mb-3">Daftar Pemakaman</h1>
                        <p>Kami bekerja sama dengan berbagai pemakaman terbaik yang tersebar di berbagai kota di Indonesia.</p>
                    </div>
                </div>
                <% cemetery.forEach((el) => { %>
                 
                    <div class="tab-content">
                        <div class="tab-pane fade show p-0 active">
                            <div class="row g-4">
                                <div class="col-lg-4 col-md-6 wow fadeInUp" data-wow-delay="0.1s">
                                    <div class="content-item rounded overflow-hidden">
                                        <div class="position-relative overflow-hidden">
                                            <a href=""><img class="img-fluid" src="<%= el.image %>" alt=""></a>
                                        </div>
                                        <div class="p-4 pb-0">
                                            <h5 class="text-primary mb-3"><%= el.name %></h5>
                                            <p><%= el.description %></p>
                                            <div class="border-top" style="padding-top: 15px;">
                                                <p><i class="fa fa-map-marker-alt text-primary me-2"></i><%= el.address %> <%= el.city %></p>
                                            </div>
                                        </div>
                                        <div class="d-flex border-top">
                                            <small class="flex-fill text-center border-end py-2"><i class="fa fa-phone text-primary me-2"></i><%= el.phone %></small>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                <% }) %>
            </div>
        </div>
        <!-- Pemakaman List End -->
        
<%- include('_footer') %>